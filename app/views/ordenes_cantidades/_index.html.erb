<h3>Productos</h3>
<div class="dataTable_wrapper">
	<table class="table table-striped table-bordered table-hover" width="100%" cellspacing="0">
		<thead>
			<tr>
				<th>Proveedor</th>
				<th>Modelo</th>
				<th>Cantidad</th>
				<th>Subtotal</th>
				<% unless @orden.fecha_pedido %>
					<th></th>
				<% end %>
			</tr>
		</thead>
		<tbody id="cantidades">
			<%= render @ordenes_cantidades %>
		</tbody>
	<table>
</div>
